{
  "name": "counter-workers",
  "version": "1.0.0",
  "description": "A counter service on Cloudflare Workers",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "check": "tsc && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "types": "wrangler types",
    "prepare": "husky",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint --fix .",
    "lint-check": "eslint .",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "counter"
  ],
  "homepage": "https://github.com/rwv/counter-workers",
  "repository": {
    "type": "git",
    "url": "https://github.com/rwv/counter-workers.git"
  },
  "author": "rwv",
  "license": "MIT",
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.6.9",
    "@cloudflare/workers-types": "^4.20250129.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.19.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.12.0",
    "@vitest/coverage-istanbul": "^3.0.4",
    "eslint": "^9.19.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vitest": "^3.0.4",
    "wrangler": "^3.107.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
}
